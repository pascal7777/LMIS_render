<% layout ('/layouts/boilerplate') %>

    <div class="container">
    <br>


        <h4>
           <strong> <%= site.site_name %></strong>
        </h4>

        <div class="row">
            <div class="col">
        <div class="container text-left">
                    <em><span style="color:rgb(106, 104, 104)">Site Information entered by: <%= site.author.username.toUpperCase()%>, 
                    <%= site.author.job_position%> (<%= site.author.affiliation%>)</span></em>
        </div>
        <br>
                <div><p>General Site information (Warehouse/stores and facilities):</p>
                    <ul>
                        <li>
                           Supply chain level: <%= site.sc_level%>
                        </li>
                        <li>
                           Location: <%= site.site_district%> - <%=site.country%>
                        </li>
                        <li>
                           Dry store capacity (M3/pallets): <%= site.site_storecap %>
                        </li>
                        <li>
                           Freezer capacity (L): <%= site.site_freezecap%>
                        </li>
                        <li>
                            Ultra-Freezer capacity (L): <%= site.site_ulfreezecap%>
                        </li>
                        <li>
                            Name Supplying warehouse: <%= site.parent_name%> 
                        </li>
                        <li>
                         # dependent stores: <%= site.children_store %>
                        </li>
                        <li>
                         # dependent facilities/SPs: <%= site.children_facility %>
                        </li>
                    </ul>
                </div>  
            
                <% if(   site.sc_level==="SP"  ){%>
                    <div><p>For Facility level (vaccination sites):</p>
                        <ul>
                            <li>
                               Catcment population: <%= site.catchment%>
                            </li>
                            <li>
                         Nr Pregnancies (last year): <%= site.pregnancies%>
                            </li>
                            <li>
                           Nr Newborns (last year): <%= site.newborns%>
                            </li>
                            <li>
                            Nr SI (0-1 yr) received 2 WHO recommended vaccinations: <%= site.survInf%>
                            </li>
                            <li>
                            EPI Target (month) for this year: <%= site.epiTarget%>
                            </li>
                            <li>
                            Outbreaks this or last year: <%= site.outbreaks%>
                            </li>
                        </ul>
                    </div>  
                    <% }%>
            
            
                    <% if( currentUser && site.author.equals(currentUser._id) || currentUser && currentUser.isAdmin) {%>
                <p><a class="btn btn-warning btn-sm" href="/sites/<%=site._id%>/edit">Correct Site Details</a>
                </p>
                <% }%>
            </div>  
    
       <div class="col">   
          <div id='map' style='width: 300px; height: 500px;'>
          </div>
       </div>
</div> 
       

       
<br>
<hr style=" width:100%; height: 10px; border: 0; box-shadow: 0 10px 10px -10px rgb(51, 51, 52) inset;">

